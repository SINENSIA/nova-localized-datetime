{
    "name": "sinensia/localizeddatetime",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova",
	"date",
	"datetime",
	"locale",
	"sinensia"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sinensia\\Localizeddatetime\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sinensia\\Localizeddatetime\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
