{
    "name": "sinensia/localizeddatetime",
    "description": "A custom localized DateTime field for Laravel Nova 4 (Laravel 10+ compatible).",
    "type": "nova-field",
    "license": "MIT",
    "authors": [
        {
            "name": "Sinensia",
            "homepage": "https://sinensia.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "laravel",
        "nova",
        "nova-field",
        "date",
        "datetime",
        "localization",
        "sinensia"
    ],
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Sinensia\\Localizeddatetime\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sinensia\\Localizeddatetime\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}